{
  "name": "vue-marquee-text-component",
  "version": "1.2.0",
  "description": "[CSS GPU Animation] Marquee Text for vuejs",
  "author": "EvodiaAut <evodia.aut@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist --target lib --name MarqueeText src/components/MarqueeText.vue && vue-cli-service build --dest docs",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/MarqueeText.common.js",
  "module": "dist/MarqueeText.common.js",
  "unpkg": "dist/MarqueeText.umd.min.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.1.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/EvodiaAut/vue-marquee-text-component/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "homepage": "https://github.com/EvodiaAut/vue-marquee-text-component",
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "vuejs2",
    "vue-component",
    "component",
    "javascript",
    "marqueetext"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvodiaAut/vue-marquee-text-component.git"
  }
}
